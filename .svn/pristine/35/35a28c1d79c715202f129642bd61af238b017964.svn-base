        <接口 name="地区列表（增量）" address="/common/dist.do" wb="张钧泳">
            <接口描述>
                1、date日期为返回字段updateTime中的最大日期，date测试数据：20181202，20181203，20181204
            </接口描述>
            <参数>
                <必填参数>
                    <param name="dw_token">接口token</param>
                    <param name="dw_source">来源</param>
                    <param name="dw_timestamp">访问时间戳</param>
                    <param name="dw_signature">访问签名</param>
                </必填参数>
                <选填参数>
                    <param name="date">上次更新日期，格式YYYYMMDD，如20181202（为空则传获取所有地区信息，否则获取改日期之后更新过的地区信息）</param>
                </选填参数>
            </参数>
            <返回值>
                <json>{"code":"状态码","msg":"状态信息",
                    "data":{"objs":[{地址信息1},{地址信息2},...]}}
                </json>
                <code>
                    <option value="0">成功</option>
                </code>
                <msg>状态信息</msg>
                <data remark="响应数据">
                    <objs remark="广告列表">
                        <districts remark="地区信息列表">
                            <attr name="distCode">行政区域代码</attr>
                            <attr name="distName">区域名称</attr>
                            <attr name="provCode">省级代码</attr>
                            <attr name="cityCode">市级代码</attr>
                            <attr name="distDegree">级别(1:省，2:市，3:县)</attr>
                            <attr name="isTerminal">是否结点(0:否，1是)</attr>
                            <attr name="distType">地区类型(1:普通,2:直辖市,3:省直管)</attr>
                            <attr name="distStatus">地区状态(0:停用，1:启用)</attr>
                            <attr name="createTime">创建时间，时间戳如1543245288000</attr>
                            <attr name="updateTime">修改时间，时间戳如1543245288000</attr>
                        </districts>
                    </objs>
                    <page remark="分页数据">
                        <attr name="pageNum">分页页码</attr>
                        <attr name="pageSize">分页显示数</attr>
                        <attr name="totalPage">总页数</attr>
                        <attr name="totalNum">总数</attr>
                    </page>
                </data>
            </返回值>
        </接口>
