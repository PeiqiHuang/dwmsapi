                </code>
                <msg>状态信息</msg>
                <data remark="响应数据">
                    <objs remark="广告列表">
                        <attr name="adId">广告id</attr>
                        <attr name="name">广告名称</attr>
                        <attr name="action">动作，http://跳转超链接 app://手机端执行</attr>
                        <attr name="cover">封面图片url</attr>
                        <attr name="adKey">广告显示位置</attr>
                        <attr name="beginTime">广告开始时间，时间戳如1543245288000</attr>
                        <attr name="endTime">广告结束时间，时间戳如1543245288000</attr>
                    </objs>
                    <page remark="分页数据">
                        <attr name="pageNum">分页页码</attr>
                        <attr name="pageSize">分页显示数</attr>
                        <attr name="totalPage">总页数</attr>
                        <attr name="totalNum">总数</attr>
                    </page>
                </data>
            </返回值>
        </接口>
